<html lang="en" ng-app="app" style="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Найти и купить авиабилеты</title>

    <!-- Bootstrap core CSS -->
    <link href="/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="~/Content/startbootstrap-agency-1.0.0/css/agency.css" rel="stylesheet"/>

    <script src="~/Content/jquery-1.11.1.min.js"></script>
    <link href="~/Content/jquery-ui-1.11.2.custom/jquery-ui.min.css" rel="stylesheet"/>
    <script src="~/Content/jquery-ui-1.11.2.custom/jquery-ui.js"></script>
    <script src="~/Content/jquery-ui-1.11.2.custom/jquery.ui.datepicker-ru.js"></script>


    <script src="/Content/bootstrap/js/bootstrap.min.js"></script>

    <script src="~/Content/moment.js"></script>

    <script src="~/Content/angular-1.3.1/angular.min.js"></script>
    <script src="~/Content/angular-1.3.1/angular-sanitize.min.js"></script>

    <script src="~/Content/ui-bootstrap-tpls-0.12.0.min.js"></script>


    @* Angular application *@
    <script src="~/Scripts/App/views.js"></script>
    <script src="~/Scripts/App/search.js"></script>
    <script src="~/Scripts/App/fares.js"></script>
    <script src="~/Scripts/App/storage.js"></script>
    <script src="~/Scripts/App/passengers.js"></script>
    <script src="~/Scripts/App/money.js"></script>
    <script src="~/Scripts/App/tickets.js"></script>
    <script src="~/Scripts/App/main.js"></script>

    @* Devices *@
    <script src="~/Scripts/App/devices.js"></script>


    <link href="~/Content/site.css" rel="stylesheet"/>

    <script src="~/Content/custom/touch-scroll/ftscroller.js"></script>
    <script src="~/Content/custom/mask/jquery.mask.js"></script>

</head>

<body class="none-selectable-text" id="body-container" ng-controller="MainController">

<div class="site-wrapper" ng-controller="ViewsController as views">

    <div class="site-wrapper-inner">

        <div class="cover-container">

            <div class="masthead clearfix">
                <div class="inner">
                    <div class="col-md-1"></div>
                    @Html.Partial("_Header")
                </div>
            </div>

            <div class="inner cover" id="inner-cover">
                @RenderBody()
                @Html.Partial("_ErrorModal")
            </div>

        </div>

    </div>

</div>

@if (!IsDebug)
{
    <style>
        body {
            cursor: none;
        }
    </style>
}

</body>
</html>
